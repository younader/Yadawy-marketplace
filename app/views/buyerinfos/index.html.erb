<p id="notice"><%= notice %></p>

<h1>Buyerinfos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buyerinfos.each do |buyerinfo| %>
      <tr>
        <td><%= link_to 'Show', buyerinfo %></td>
        <td><%= link_to 'Edit', edit_buyerinfo_path(buyerinfo) %></td>
        <td><%= link_to 'Destroy', buyerinfo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Buyerinfo', new_buyerinfo_path %>
